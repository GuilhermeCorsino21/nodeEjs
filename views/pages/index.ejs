<!DOCTYPE html> 
<!--SINTAXE EJS INCLUINDO O HEAD NO INDEX-->
<%- include('../partials/head'); %>

    <body class="container">
<!--SINTAXE EJS INCLUINDO O HEADER NO INDEX-->        
    <%- include('../partials/header', {pagina: 'Home'}); %>         
        <main>
            <div class="jumbotron">
                <h1>Node com EJS</h1>
                <p>Aprendendo arquivos EJS</p>
                <p><%= subtitle %></p>
            </div>
            <ul>
                <% qualitys.forEach(function(quality){ %>
                        <!-- QUANDO QUERO PRINTAR NA TELA EU USO O = -->
                    <li>
                        <strong><%= quality.title %></strong> <%= quality.message %>
                    </li>

                <% }) %> 
                
                        
            </ul>

        </main>
<!--SINTAXE EJS INCLUINDO O FOOTER NO INDEX-->
    <%- include('../partials/footer'); %>
        
    </body>
</html>